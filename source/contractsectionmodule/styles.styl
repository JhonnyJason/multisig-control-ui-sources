// contractsection styles
#contractsection
    margin auto
    margin-top 20px
    padding 20px
    max-width: 900px
    box-sizing: border-box
    border solid 2px cd-flash
    border-radius: 5px
    h1
        margin 12px 0
        padding 0

#address-display
    background-color: cd-flash
    color cd-dark

#status-message-display
    margin-top 7px
    color red

#change-contract-button
    border solid 2px cd-flash
    padding 10px
    margin-top 12px
    color cd-flash